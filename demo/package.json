{
  "name": "demo",
  "version": "5.0.5-alpha.2",
  "private": true,
  "type": "module",
  "devDependencies": {
    "@iwsio/fetch": "^2.1.5",
    "@iwsio/forms": "workspace:*",
    "@tailwindcss/typography": "^0.5.19",
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/react-query": "^5.90.19",
    "@tanstack/react-query-devtools": "^5.91.2",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/luxon": "^3.7.1",
    "@types/node": "^25.0.10",
    "@types/prismjs": "^1.26.5",
    "@types/react": "^19.2.9",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "classnames": "^2.5.1",
    "daisyui": "^5.5.14",
    "html-react-parser": "^5.2.11",
    "jsdom": "^27.4.0",
    "luxon": "^3.7.2",
    "markdown-it": "^14.1.0",
    "npm-run-all": "^4.1.5",
    "prismjs": "^1.30.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-router": "^7.12.0",
    "rimraf": "^6.1.2",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vitest": "^4.0.17",
    "web-vitals": "^5.1.0"
  },
  "scripts": {
    "prestart": "rimraf dist && npm run generate",
    "start": "VITE_GITHUB_SHA=`git rev-parse HEAD` vite dev",
    "prebuild": "npm run generate",
    "build": "vite build --mode production",
    "clean": "npm-run-all -s clean:ts clean:fs",
    "clean:ts": "tsc --build --clean",
    "clean:fs": "rimraf dist",
    "typecheck": "tsc --build",
    "lint": "eslint src",
    "test": "VITEST_SEGFAULT_RETRY=3 vitest --run --passWithNoTests",
    "pregenerate": "rimraf public/content && mkdir -p public/content",
    "generate": "node src/bin/generate.mjs",
    "serve": "serve -s -n --no-etag -l tcp://0.0.0.0:3000 -c ../serve.json ./dist"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "main": "npm run serve",
  "author": "",
  "license": "ISC",
  "dependencies": {
    "serve": "^14.2.5"
  }
}
